<!DOCTYPE html>
<html>
<head>
{% block head %}
  <title>{% block title %}Документ{% endblock %}</title>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <link rel="shortcut icon" href="/img/favicon.ico" type="image/x-icon">
  <link rel="stylesheet" href="{{ asset('bundles/acmedemo/css/demo.css') }}" />
  <link href="{{ asset('bundles/totalcandocumancer/css/bootstrap-responsive.min.css') }}" rel="stylesheet" type="text/css" />
  <link href="{{ asset('bundles/totalcandocumancer/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
  <link href='http://fonts.googleapis.com/css?family=PT+Sans+Narrow&subset=latin,cyrillic' rel='stylesheet' type='text/css'>
{% endblock %}
</head>
<body>
{% block body %}
{% for flashMessage in app.session.flashbag.get('notice') %}
    <div class="flash-message">
        <em>Notice</em>: {{ flashMessage }}
    </div>
{% endfor %}
{% block navigation %}{% endblock %}
<br>
<section>
<div class="gyro-unit text-center">
    <div class="text-left">
{% block content %}{% endblock %}
    </div>
</div>
</section>
<footer>
{% block footer %}
<div class="footer">
  <div>
    <center><p>{{ "now"|date("Y") }}</p></center>
  </div>
</div>
{% endblock %}
</footer>

<script type="text/javascript">
    var BRAVOREGCOM = {
            'auth_url': '{{ path('super_auth')}}',
            'user_id': {% if app.user.exId is defined and app.user.exId%}{{app.user.exId}}{%else%}0{% endif %},
            'callback': function(){ $('.signin').addClass('btn btn-success'); $('.bravoreg-link').addClass('btn btn-warning'); $('.signout').addClass('btn btn-danger'); $('.bravoreg b').addClass('btn'); $('.bravoreg').first().addClass('btn-group');},
            'signout_url': '{{ path('super_logout')}}',
            'auto': false
        };
</script>
<link rel="stylesheet" type="text/css" href="http://bravoreg.com/widget/css/widget.css" />
<script type="text/javascript" src="http://bravoreg.com/widget/js/widget.js"></script>
<script src="{{ asset('bundles/totalcandocumancer/js/jquery-2.0.3.min.js') }}"></script>
<script src="{{ asset('bundles/totalcandocumancer/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/totalcandocumancer/js/app.js') }}"></script>
{% endblock %}
</body>
</html>
